{
	"name": "fast-element-plus",
	"version": "1.0.0-alpha.11",
	"description": "Fast 组件库.",
	"type": "module",
	"keywords": [
		"fast-element-plus",
		"fast",
		"typescript",
		"component library",
		"ui framework",
		"ui",
		"vite",
		"vue"
	],
	"license": "Apache-2.0",
	"homepage": "https://fastdotnet.com",
	"bugs": {
		"url": "https://gitee.com/China-xiaoFang/fast.element.plus/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://gitee.com/China-xiaoFang/fast.element.plus"
	},
	"author": "小方",
	"files": [
		"./Fast.png",
		"./LICENSE",
		"./README.md",
		"./README.zh.md",
		"./dist",
		"./es",
		"./global.d.ts",
		"./lib",
		"./styles",
		"./types"
	],
	"main": "lib/index.js",
	"module": "es/index.mjs",
	"types": "es/index.d.ts",
	"exports": {
		".": {
			"types": "./es/index.d.ts",
			"require": "./lib/index.js",
			"import": "./es/index.mjs"
		},
		"./global": {
			"types": "./global.d.ts"
		},
		"./es": {
			"types": "./es/index.d.ts",
			"import": "./es/index.mjs"
		},
		"./lib": {
			"types": "./lib/index.d.ts",
			"require": "./lib/index.js"
		},
		"./es/*.mjs": {
			"types": "./es/*.d.ts",
			"import": "./es/*.mjs"
		},
		"./es/*": {
			"types": [
				"./es/*.d.ts",
				"./es/*/index.d.ts"
			],
			"import": "./es/*.mjs"
		},
		"./lib/*.js": {
			"types": "./lib/*.d.ts",
			"require": "./lib/*.js"
		},
		"./lib/*": {
			"types": [
				"./lib/*.d.ts",
				"./lib/*/index.d.ts"
			],
			"require": "./lib/*.js"
		},
		"./*": "./*"
	},
	"style": "dist/index.css",
	"sideEffects": false,
	"unpkg": "dist/index.iife.min.js",
	"jsdelivr": "dist/index.iife.min.js",
	"publishConfig": {
		"access": "public"
	},
	"peerDependencies": {
		"@element-plus/icons-vue": "^2.3",
		"@fast-china/utils": "^1",
		"@fast-element-plus/icons-vue": "^1.0.17",
		"element-plus": "^2",
		"vue": "^3.5"
	},
	"dependencies": {
		"@types/lodash": "^4.17",
		"@types/lodash-es": "^4.17",
		"@types/sortablejs": "^1.15",
		"@vueuse/core": "^13",
		"decimal.js": "^10",
		"lodash": "^4.17",
		"lodash-es": "^4.17",
		"lodash-unified": "^1",
		"screenfull": "^6",
		"sortablejs": "^1.15"
	},
	"devDependencies": {
		"@element-plus/icons-vue": "^2.3",
		"@fast-china/eslint-config": "^1",
		"@fast-china/utils": "^1",
		"@fast-element-plus/icons-vue": "^1.0.17",
		"@rollup/plugin-terser": "^0.4",
		"@types/node": "^24",
		"@vitejs/plugin-vue": "^5",
		"@vitejs/plugin-vue-jsx": "^4",
		"@vue/tsconfig": "^0.7",
		"element-plus": "^2",
		"eslint": "^9",
		"prettier": "^3",
		"sass": "^1.89",
		"terser": "^5",
		"typescript": "^5",
		"vite": "^6",
		"vite-plugin-dts": "^4",
		"vue": "^3.5",
		"vue-tsc": "^2"
	},
	"browserslist": {
		"production": [
			"> 1%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	}
}
