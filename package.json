{
	"name": "fast-element-plus",
	"author": "小方",
	"version": "1.0.0",
	"description": "Fast 组件库.",
	"keywords": [
		"fast-element-plus",
		"fast",
		"typescript",
		"component library",
		"ui framework",
		"ui",
		"vite",
		"vue"
	],
	"license": "Apache-2.0",
	"publishConfig": {
		"access": "public"
	},
	"homepage": "https://fastdotnet.com",
	"repository": {
		"type": "git",
		"url": "https://gitee.com/China-xiaoFang/fast.element.plus"
	},
	"bugs": {
		"url": "https://gitee.com/China-xiaoFang/fast.element.plus/issues"
	},
	"type": "module",
	"workspaces": [
		"packages/*"
	],
	"scripts": {
		"check": "vue-tsc",
		"update:version": "tsx scripts/update-version.ts",
		"build": "tsx scripts/build.ts && vue-tsc && vite build && tsx scripts/move-file.ts",
		"publish": "cd fast-element-plus && npm publish",
		"lint:eslint": "eslint --max-warnings 0 \"packages/**/*.{vue,js,ts,tsx}\" --fix",
		"lint:prettier": "prettier --write \"packages/**/*.{vue,js,ts,tsx,json,css,less,scss,html,md}\"",
		"lint": "pnpm lint:eslint && pnpm lint:prettier"
	},
	"dependencies": {
		"@vueuse/core": "^10.11.0",
		"crypto-js": "^4.2.0",
		"decimal.js": "^10.4.3",
		"lodash-es": "^4.17.21",
		"pinia-plugin-persistedstate": "^4.0.2",
		"screenfull": "^5.2.0",
		"sortablejs": "^1.15.2"
	},
	"devDependencies": {
		"@element-plus/icons-vue": "^2.3.1",
		"@fast-element-plus/build": "workspace:*",
		"@fast-element-plus/components": "workspace:*",
		"@fast-element-plus/constants": "workspace:*",
		"@fast-element-plus/directives": "workspace:*",
		"@fast-element-plus/hooks": "workspace:*",
		"@fast-element-plus/icons-vue": "^1.0.4",
		"@fast-element-plus/stores": "workspace:*",
		"@fast-element-plus/utils": "workspace:*",
		"@types/crypto-js": "^4.2.0",
		"@types/lodash-es": "^4.17.12",
		"@types/node": "*",
		"@types/sortablejs": "^1.15.8",
		"@typescript-eslint/eslint-plugin": "^8.6.0",
		"@typescript-eslint/parser": "^8.6.0",
		"@vitejs/plugin-vue": "^5.1.4",
		"@vitejs/plugin-vue-jsx": "^4.0.0",
		"@vue/tsconfig": "^0.5.1",
		"axios": "^1.7.2",
		"element-plus": "^2.8.4",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-define-config": "^2.1.0",
		"eslint-plugin-import": "^2.30.0",
		"eslint-plugin-prettier": "^5.2.1",
		"eslint-plugin-vue": "^9.28.0",
		"pinia": "^2.1.7",
		"prettier": "^3.3.3",
		"sass": "^1.79.3",
		"terser": "^5.33.0",
		"tsx": "^4.19.1",
		"typescript": "^5.6.2",
		"vite": "^5.4.7",
		"vite-plugin-dts": "^4.2.1",
		"vue": "^3.4.27",
		"vue-eslint-parser": "^9.4.3",
		"vue-router": "^4.2.2",
		"vue-tsc": "^2.1.6"
	},
	"engines": {
		"node": ">=20.0.0"
	},
	"browserslist": {
		"production": [
			"> 1%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	}
}
