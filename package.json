{
	"name": "fast-element-plus",
	"version": "1.0.0",
	"description": "Fast 组件库.",
	"type": "module",
	"keywords": [
		"fast-element-plus",
		"fast",
		"typescript",
		"component library",
		"ui framework",
		"ui",
		"vite",
		"vue"
	],
	"license": "Apache-2.0",
	"homepage": "https://fastdotnet.com",
	"bugs": {
		"url": "https://gitee.com/China-xiaoFang/fast.element.plus/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://gitee.com/China-xiaoFang/fast.element.plus"
	},
	"author": "小方",
	"publishConfig": {
		"access": "public"
	},
	"workspaces": [
		"packages/*"
	],
	"scripts": {
		"check": "vue-tsc",
		"update:version": "tsx scripts/update-version.ts",
		"build:styles": "sass ./styles/index.scss:./fast-element-plus/dist/styles/index.css",
		"build:move": "pnpm run build:styles && tsx scripts/move-file.ts",
		"build": "tsx scripts/build.ts && vue-tsc && vite build --config vite.umd-iife.config.ts && vite build --config vite.es-lib.config.ts",
		"publish": "cd fast-element-plus && npm publish",
		"lint:eslint": "eslint --max-warnings 0 \"packages/**/*.{vue,js,ts,tsx}\" --fix",
		"lint:prettier": "prettier --write \"packages/**/*.{vue,js,ts,jsx,tsx,md,json,css,scss,less,html}\"",
		"lint": "pnpm lint:eslint && pnpm lint:prettier"
	},
	"dependencies": {
		"@types/lodash": "^4.17",
		"@types/lodash-es": "^4.17",
		"@types/sortablejs": "^1.15",
		"@vueuse/core": "^13",
		"decimal.js": "^10",
		"lodash": "^4.17",
		"lodash-es": "^4.17",
		"lodash-unified": "^1",
		"screenfull": "^6",
		"sortablejs": "^1.15"
	},
	"devDependencies": {
		"@element-plus/icons-vue": "^2.3",
		"@fast-china/eslint-config": "^1",
		"@fast-china/utils": "^1",
		"@fast-element-plus/components": "workspace:*",
		"@fast-element-plus/constants": "workspace:*",
		"@fast-element-plus/directives": "workspace:*",
		"@fast-element-plus/hooks": "workspace:*",
		"@fast-element-plus/icons-vue": "^1.0.17",
		"@rollup/plugin-terser": "^0.4",
		"@types/node": "^24",
		"@vitejs/plugin-vue": "^6",
		"@vitejs/plugin-vue-jsx": "^5",
		"@vue/tsconfig": "^0.8",
		"element-plus": "^2",
		"eslint": "^9",
		"prettier": "^3",
		"sass": "^1",
		"terser": "^5",
		"typescript": "^5",
		"vite": "^7",
		"vite-plugin-dts": "^4",
		"vue": "^3.5",
		"vue-tsc": "^3"
	},
	"engines": {
		"node": ">=20.0.0"
	},
	"browserslist": {
		"production": [
			"> 1%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	}
}
